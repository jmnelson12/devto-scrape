{
    "version": 1,
    "name": "devto-scrape",
    "builds": [{
            "src": "server.js",
            "use": "@now/node-server"
        },
        {
            "src": "package.json",
            "use": "@now/next"
        }
    ],
    "public": true,
    "routes": [{
        "src": "/(.*)",
        "dest": "server.js"
    }]
}
